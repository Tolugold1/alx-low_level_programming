CC=gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm

.PHONY: all
all: $(OBJ)
	$(CC) $< -o $(NAME)
  
.PHONY: clean
clean: *~ $(NAME)
	$(RM) -f *~ $(NAME)

.PHONY: oclean
oclean: $(OBJ)
	$(RM) -f $(OBJ)

.PHONY: fclean
fclean: clean oclean

.PHONY: re
re: oclean all
  
